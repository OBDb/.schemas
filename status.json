{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
      "testers": {
        "type": "object",
        "patternProperties": {
          "^[0-9]{4}$": {
            "type": "object",
            "properties": {
              "username": {
                "type": "string"
              },
              "onboarded": {
                "type": "string",
                "format": "date"
              }
            },
            "required": ["username", "onboarded"]
          }
        },
        "additionalProperties": false
      }
    },
    "required": ["testers"]
  }